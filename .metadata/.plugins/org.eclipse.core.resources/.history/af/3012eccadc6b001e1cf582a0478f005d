<mapper namespace="boardMapper">
		
		<!-- 
		resultType이 "map"인 경우 
		K : 컬럼명(BOARD_CODE, BOARD_NAME)
		V : 컬럼 값( 1, 공지사항)
		
		[{Board_CODE = 1, BOARD_NAME}..........] 
		
		
		 -->
		
		
		
		
		
		<select id="selectBoardTypeList" resultType="map">
			SELECT * FROM "BOARD_TYPE" ORDER BY 1
		</select>


</mapper>